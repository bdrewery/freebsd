/dts-v1/;
/plugin/;

/ {
	compatible = "allwinner,sun8i-h3";
};

&spi0 {
	status = "okay";
	spigen0: spigen0 {
		compatible = "freebsd,spigen";
		reg = <0>;
		spi-max-frequency = <500000>; /* Req'd property, override with spi(8) */
		status = "okay";
	};
};

&spi1 {
	status = "okay";
	spigen1: spigen1 {
		compatible = "freebsd,spigen";
		reg = <1>;
		spi-max-frequency = <500000>; /* Req'd property, override with spi(8) */
		status = "okay";
	};
};
